<?xml version="1.0" encoding="UTF-8"?>
<mxboard>
    <mongo-conf>
        <host>127.0.0.1</host>
        <port>27017</port>
        <task-db>task</task-db>
        <train-db>train</train-db>
        <test-db>test</test-db>
    </mongo-conf>
    <rpc-conf>
        <host>127.0.0.1</host>
        <port>50051</port>
        <max-thread-num>4</max-thread-num>
        <one-day-time-in-seconds>86400</one-day-time-in-seconds>
    </rpc-conf>
    <log-conf>
        <log-format>%(asctime)s - %(filename)s-%(lineno)s[%(levelname)s]: %(message)s</log-format>
        <log-max-bytes>10485760</log-max-bytes>
        <log-backup-count>5</log-backup-count>
        <log-level>INFO</log-level>
    </log-conf>
    <storage-conf>
        <symbol-json-root></symbol-json-root>
        <params-root></params-root>
    </storage-conf>
    <task-queue-conf>
        <queue-max-size>10</queue-max-size>
        <wait-time-out>10</wait-time-out>
    </task-queue-conf>
    <data-download-conf>
        <max-retry-num>5</max-retry-num>
    </data-download-conf>
</mxboard>