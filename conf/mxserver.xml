<?xml version="1.0" encoding="UTF-8"?>
<mxboard>
    <mxnet-conf>
        <rcnn-path>rcnn_path_in_your_computer</rcnn-path>
    </mxnet-conf>
    <mongo-conf>
        <host>127.0.0.1</host>
        <port>27017</port>
        <basic-db>basic</basic-db>
        <task-db>task</task-db>
        <train-db>train</train-db>
        <test-db>test</test-db>
    </mongo-conf>
    <rpc-conf>
        <host>127.0.0.1</host>
        <port>50051</port>
        <max-thread-num>4</max-thread-num>
        <one-day-time-in-seconds>86400</one-day-time-in-seconds>
    </rpc-conf>
    <zk-conf>
        <use-zk>False</use-zk>
        <zk-timeout>5</zk-timeout>
        <zk-host>127.0.0.1:2181</zk-host>
        <zk-host>127.0.0.1:2182</zk-host>
        <zk-host>127.0.0.1:2183</zk-host>
    </zk-conf>
    <log-conf>
        <log-file-root>/home/panyiming/wuzhenan/open_source_projects/mxserver/log/</log-file-root>
        <log-format>%(asctime)s - %(filename)s-%(lineno)s[%(levelname)s]: %(message)s</log-format>
        <log-max-bytes>10485760</log-max-bytes>
        <log-backup-count>5</log-backup-count>
        <log-level>INFO</log-level>
    </log-conf>
    <storage-conf>
        <rec-root>../data_center/classification</rec-root>
        <symbol-json-root>../model_zoo/</symbol-json-root>
        <params-root>../params_zoo</params-root>
    </storage-conf>
    <task-queue-conf>
        <queue-max-size>10</queue-max-size>
        <wait-time-out>10</wait-time-out>
    </task-queue-conf>
    <data-download-conf>
        <max-retry-num>5</max-retry-num>
    </data-download-conf>
</mxboard>